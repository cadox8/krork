<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>net.athonedevs</groupId>
    <artifactId>Krork</artifactId>
    <version>0.7.2-Alpha</version>
    <packaging>jar</packaging>

    <name>Krork</name>
    <description>A pure Java engine</description>
    <url>https://cadox8.github.io/krork</url>

    <organization>
        <name>AthoneDevs</name>
        <url>https://athonedevs.github.io</url>
    </organization>

    <developers>
        <developer>
            <name>Cadox8</name>
            <email>cadox8@gmail.com</email>
            <organization>AthoneDevs</organization>
            <organizationUrl>https://athonedevs.github.io</organizationUrl>
            <properties>
                <picUrl>https://cadox8.github.io/img/cadox.png</picUrl>
            </properties>
        </developer>
        <developer>
            <name>Wikijito7</name>
            <email>wikyfg@gmail.com</email>
            <organization>AthoneDevs</organization>
            <organizationUrl>https://athonedevs.github.io</organizationUrl>
        </developer>
    </developers>

    <licenses>
        <license>
            <name>Private License</name>
            <url>LICENSE.md</url>
            <distribution>source</distribution>
        </license>
    </licenses>

    <scm>
        <connection>scm:git:git://github.com/cadox8/krork</connection>
        <developerConnection>scm:git:git://github.com/cadox8/krork</developerConnection>
        <tag>HEAD</tag>
        <url>scm:git:git://github.com/cadox8/krork</url>
    </scm>

    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
    </properties>
    <build>
        <sourceDirectory>src</sourceDirectory>
        <resources>
            <resource>
                <targetPath>.</targetPath>
                <filtering>true</filtering>
                <directory>src</directory>
                <includes>
                    <include>*.yml</include>
                </includes>
            </resource>
        </resources>
        <finalName>${project.artifactId}</finalName>
    </build>
    <repositories>
    </repositories>
    <dependencies>
        <!-- Lombok Repo -->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <version>1.18.6</version>
        </dependency>

        <!-- Gson Repo -->
        <dependency>
            <groupId>com.google.code.gson</groupId>
            <artifactId>gson</artifactId>
            <version>2.8.5</version>
        </dependency>

        <!-- Reflection Repo -->
        <dependency>
            <groupId>org.reflections</groupId>
            <artifactId>reflections</artifactId>
            <version>0.9.11</version>
        </dependency>

        <!-- Commons Repo -->
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.8.1</version>
        </dependency>

    </dependencies>
</project>